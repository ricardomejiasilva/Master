
public class ThreadBasicsRunner {

	public static void main(String[] args) {
		// Task 1
		for (int i = 101; i <= 199; i++) {
			System.out.println(i + " ");
		}

		// Task 2

		for (int i = 201; i <= 299; i++) {
			System.out.println(i + " ");
		}

		// Task 3

		for (int i = 301; i <= 399; i++) {
			System.out.println(i + " ");
		}

	}

}
